<div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "Cars")}", backend_cars_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Cars Model")}", backend_car_models_path %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New Car")}", backend_brandnew_cars_path %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "User View")}", cars_path %></li>
    </ul>
