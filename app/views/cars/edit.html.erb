<div class="block">
  <%= render "header" ,:locals => {cname:'edit'} %>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> Car</h2>
    <div class="inner">
      <%= form_for @car, :url => car_path(@car), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
