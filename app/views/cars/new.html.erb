<div class="block">
  <%= render "header", :locals => { :cname => "new" } %>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.new", :default => "New")%>  Car</h2>
    <div class="inner">
      <%= form_for :car, :url => cars_path, :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
